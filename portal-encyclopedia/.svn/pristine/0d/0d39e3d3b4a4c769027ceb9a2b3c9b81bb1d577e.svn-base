<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE taglib
        PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
        "http://java.sun.com/j2ee/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
  <tlib-version>1.0</tlib-version>
  <jsp-version>1.2</jsp-version>
  <short-name>ad</short-name>
  <uri>http://com.wangku.wsa/ad-taglib</uri>
  <description>
    	后台管理自定义标签
  </description>

  <tag>
    <name>getAdvertisesHtml</name>
    <tag-class>com.wangku.wsa.system.admin.taglib.AdvertisTaglib</tag-class>
    <description>广告标签</description>
    <attribute>
       <name>code</name>
       <required>true</required>
       <type>string</type>
    </attribute>
  </tag>

</taglib>
